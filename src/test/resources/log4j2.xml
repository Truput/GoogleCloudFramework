<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
    <Appenders>
        <Console name="Console">
            <PatternLayout pattern="%d %p %c{2} [%t] %l %m%n"/>
        </Console>
        <File name="File" fileName="target/logs/google-cloud.log">
            <PatternLayout pattern="%d %p %c{2} [%t] %m%n"/>
        </File>
    </Appenders>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="Console"/>
            <AppenderRef ref="File"/>
        </Root>
    </Loggers>
</Configuration>